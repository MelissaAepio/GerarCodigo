{
    "version": 2,
    "builds": [
      { "src": "api/buscar-codigo.js", "use": "@vercel/node" },
      { "src": "index.html", "use": "@vercel/static" }
    ],
    "routes": [
      { "src": "/api/buscar-codigo", "dest": "/api/buscar-codigo.js" },
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }
  